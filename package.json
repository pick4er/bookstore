{
  "name": "bookstore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "rm -rf ./build && rm -rf ./lib",
    "compile": "babel webpack-configs -d lib",
    "prepare": "npm run clear && npm run compile",
    "build-dev-server": "webpack --config lib/server/dev.js",
    "build-dev-client": "webpack --config lib/client/dev.js",
    "build-prod-server": "webpack --config lib/server/prod.js",
    "build-prod-client": "webpack --config lib/client/prod.js",
    "start": "node build/server.js",
    "dev": "export NODE_ENV=development && npm run prepare && npm run build-dev-client && npm run build-dev-server && npm run start",
    "prod": "export NODE_ENV=production && npm run prepare && npm run build-prod-client && npm run build-prod-server && npm run start",
    "heroku-postbuild": "export NODE_ENV=production && npm run build-prod-client && npm run build-prod-server"
  },
  "author": "pick4er",
  "license": "WTFPL",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
